#!/bin/bash

session=$(tmux display-message -p '#S')
if [ $? -eq 1 ]; then
  tmux
  session=$(tmux display-message -p '#S')
fi
echo $session
tmux split-window -v -p 20 -t $session.0
tmux split-window -h -p 30 -t $session.0
tmux split-window -v -p 50 -t $session.1
tmux split-window -v -p 50 -t $session.2

